<%= render 'errors' %><br>

<div id="form">
<%= form_with(model: @user, url:'/signup', local: true) do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %><br>
    <%= f.label :email %>
    <%= f.text_field :email %><br>
    <%= f.label :password %>
    <%= f.password_field :password %><br>
    <%= f.submit "Sign Up"%>
<% end %>
</div>